{
  "name": "site",
  "description": "Primary website for the dsmJS user-group",
  "license": "MIT",
  "author": "dsmJS",
  "maintainers": [
    "Matt Travi <npm@travi.org> (https://matt.travi.org/)"
  ],
  "contributors": [
    "Dustin Schau <dustinschau@gmail.com> (https://dustinschau.com)"
  ],
  "private": true,
  "repository": "dsmjs/site",
  "bugs": "https://github.com/dsmjs/site/issues",
  "homepage": "http://dsmjs.netlify.com/",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . --cache",
    "test": "run-s lint:* build",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "start": "run-s develop",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "deploy": "run-p --print-label deploy:*",
    "deploy:netlify": "netlify deploy --site-id $NETLIFY_SITE_ID --access-token $NETLIFY_ACCESS_TOKEN --path ./public",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "1.6.18",
    "commitlint-config-dsmjs": "1.0.3",
    "core-js": "2.5.7",
    "cz-conventional-changelog": "2.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.15.1",
    "husky": "1.1.0",
    "markdownlint-cli": "0.13.0",
    "netlify-cli": "1.2.3",
    "npm-run-all": "4.1.3"
  },
  "dependencies": {
    "@dsmjs/components": "2.2.0",
    "gatsby": "^2.0.0",
    "gatsby-link": "^2.0.0",
    "gatsby-plugin-glamor": "^2.0.0",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-remark-autolink-headers": "^2.0.0",
    "gatsby-remark-prismjs": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.0",
    "gatsby-transformer-remark": "^2.0.0",
    "gatsby-transformer-yaml": "2.1.1",
    "glamor": "2.20.40",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0"
  }
}

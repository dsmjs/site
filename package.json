{
  "name": "site",
  "description": "Primary website for the dsmJS user-group",
  "license": "MIT",
  "author": "dsmJS",
  "maintainers": [
    "Matt Travi <npm@travi.org> (https://matt.travi.org/)"
  ],
  "contributors": [
    "Dustin Schau <dustinschau@gmail.com> (https://dustinschau.com)",
    "Michael Leners <micleners@gmail.com> (https://micleners.com/)"
  ],
  "private": true,
  "repository": "dsmjs/site",
  "bugs": "https://github.com/dsmjs/site/issues",
  "homepage": "https://site.dsmjs.com/",
  "scripts": {
    "lint:js": "eslint . --cache",
    "lint:md": "remark --frail --quiet .",
    "lint:peer": "npm ls >/dev/null",
    "test": "run-s lint:* build",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "start": "run-s develop",
    "presnapshot": "run-s build",
    "snapshot": "npx percy snapshot public/",
    "deploy": "run-p --print-label deploy:*",
    "predeploy:netlify": "run-s build",
    "deploy:netlify": "netlify deploy --dir=./public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@dsmjs/eslint-config": "1.0.59",
    "@dsmjs/eslint-config-react": "1.1.54",
    "@dsmjs/remark-preset-lint": "1.0.12",
    "commitlint-config-dsmjs": "1.0.22",
    "cz-conventional-changelog": "3.3.0",
    "greenkeeper-lockfile": "1.15.1",
    "husky": "5.0.6",
    "netlify-cli": "2.70.0",
    "npm-run-all": "4.1.5",
    "remark": "13.0.0",
    "remark-cli": "8.0.1",
    "remark-frontmatter": "2.0.0"
  },
  "dependencies": {
    "@dsmjs/components": "5.2.29",
    "gatsby": "2.29.3",
    "gatsby-link": "2.8.0",
    "gatsby-plugin-glamor": "2.7.0",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-netlify": "2.8.0",
    "gatsby-plugin-react-helmet": "3.7.0",
    "gatsby-remark-autolink-headers": "2.8.0",
    "gatsby-remark-prismjs": "3.10.0",
    "gatsby-source-filesystem": "2.8.1",
    "gatsby-transformer-remark": "2.13.1",
    "glamor": "2.20.40",
    "prismjs": "1.23.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "6.1.0"
  }
}

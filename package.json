{
  "name": "site",
  "description": "Primary website for the dsmJS user-group",
  "license": "MIT",
  "author": "dsmJS",
  "maintainers": [
    "Matt Travi <npm@travi.org> (https://matt.travi.org/)"
  ],
  "contributors": [
    "Dustin Schau <dustinschau@gmail.com> (https://dustinschau.com)",
    "Michael Leners <micleners@gmail.com> (https://micleners.com/)"
  ],
  "private": true,
  "repository": "dsmjs/site",
  "bugs": "https://github.com/dsmjs/site/issues",
  "homepage": "https://site.dsmjs.com/",
  "scripts": {
    "lint:js": "eslint . --cache",
    "lint:md": "remark --frail --quiet .",
    "lint:peer": "npm ls >/dev/null",
    "test": "run-s lint:* build",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "start": "run-s develop",
    "snapshot": "npx percy snapshot public/",
    "deploy": "run-p --print-label deploy:*",
    "deploy:netlify": "netlify deploy --dir=./public --prod"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@dsmjs/eslint-config": "1.0.37",
    "@dsmjs/eslint-config-react": "1.1.32",
    "@dsmjs/remark-preset-lint": "1.0.8",
    "commitlint-config-dsmjs": "1.0.17",
    "cz-conventional-changelog": "3.2.0",
    "greenkeeper-lockfile": "1.15.1",
    "husky": "4.2.5",
    "netlify-cli": "2.54.0",
    "npm-run-all": "4.1.5",
    "remark": "12.0.0",
    "remark-cli": "8.0.0",
    "remark-frontmatter": "2.0.0"
  },
  "dependencies": {
    "@dsmjs/components": "5.2.0",
    "gatsby": "2.19.12",
    "gatsby-link": "2.4.2",
    "gatsby-plugin-glamor": "2.3.4",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-netlify": "2.3.5",
    "gatsby-plugin-react-helmet": "3.3.4",
    "gatsby-remark-autolink-headers": "2.3.6",
    "gatsby-remark-prismjs": "3.5.5",
    "gatsby-source-filesystem": "2.3.12",
    "gatsby-transformer-remark": "2.8.17",
    "glamor": "2.20.40",
    "prismjs": "1.20.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0"
  }
}

{
  "name": "site",
  "description": "Primary website for the dsmJS user-group",
  "license": "MIT",
  "author": "dsmJS",
  "maintainers": [
    "Matt Travi <npm@travi.org> (https://matt.travi.org/)"
  ],
  "contributors": [
    "Dustin Schau <dustinschau@gmail.com> (https://dustinschau.com)"
  ],
  "private": true,
  "repository": "dsmjs/site",
  "bugs": "https://github.com/dsmjs/site/issues",
  "homepage": "http://dsmjs.netlify.com/",
  "scripts": {
    "lint:js": "eslint . --cache",
    "test": "run-s lint:* build",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "start": "run-s develop",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "deploy": "run-p --print-label deploy:*",
    "deploy:netlify": "netlify deploy --site=$NETLIFY_SITE_ID --auth=$NETLIFY_ACCESS_TOKEN --dir=./public --prod"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "1.7.0",
    "commitlint-config-dsmjs": "1.0.6",
    "core-js": "2.6.5",
    "cz-conventional-changelog": "2.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.15.1",
    "husky": "1.3.1",
    "markdownlint-cli": "0.13.0",
    "netlify-cli": "2.7.0",
    "npm-run-all": "4.1.5"
  },
  "dependencies": {
    "@dsmjs/components": "2.2.4",
    "gatsby": "2.1.7",
    "gatsby-link": "2.0.11",
    "gatsby-plugin-glamor": "2.0.8",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-react-helmet": "3.0.6",
    "gatsby-remark-autolink-headers": "2.0.14",
    "gatsby-remark-prismjs": "3.2.4",
    "gatsby-source-filesystem": "2.0.20",
    "gatsby-transformer-remark": "2.2.5",
    "glamor": "2.20.40",
    "prismjs": "1.15.0",
    "prop-types": "15.7.2",
    "react": "^16.8.2",
    "react-dom": "16.8.2",
    "react-helmet": "5.2.0"
  }
}
